Algoritmo "Cálculos Simples"

Var
 n1,n2,so,di,pr,me: real

Inicio
 escreva("Entre com o primeiro número: ")
   leia(n1)
 escreva("Entre com o primeiro número: ")
   leia(n2)
    so<-(n1+n2)
    di<-(n1-n2)
    pr<-(n1*n2)
    me<-((n1+n2)/2)
      escreval("Soma: ",so)
      escreval("Diferença: ",di)
      escreval("Produto: ",pr)
      escreval("Média: ",me)

Fimalgoritmo